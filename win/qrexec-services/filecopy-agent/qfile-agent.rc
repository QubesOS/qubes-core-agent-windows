#include <windows.h>

VS_VERSION_INFO VERSIONINFO
 FILEVERSION 1,0,0,0
 PRODUCTVERSION 1,0,0,0
 FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
#ifdef _DEBUG
 FILEFLAGS VS_FF_DEBUG
#else
 FILEFLAGS 0x0L
#endif
 FILEOS VOS_NT_WINDOWS32
 FILETYPE VFT_APP
 FILESUBTYPE VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "000904b0"
        BEGIN
            VALUE "CompanyName", "Invisible Things Lab"
            VALUE "FileDescription", "QFile agent"
            VALUE "FileVersion", "1, 0, 0, 0"
            VALUE "InternalName", "qfile_agent"
            VALUE "LegalCopyright", "See individual source files"
            VALUE "OriginalFilename", "qfile_agent.exe"
            VALUE "ProductName", "Qubes OS"
            VALUE "ProductVersion", "1, 0, 0, 0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x9, 1200
    END
END

// Must reference a manifest for visual styles and elevation
// Oh, and it must happen here, or MinGW will ignore it!
#if defined(__MINGW32__) || defined(__CYGWIN__)
CREATEPROCESS_MANIFEST_RESOURCE_ID RT_MANIFEST "qfile-agent.exe.manifest"
#endif
